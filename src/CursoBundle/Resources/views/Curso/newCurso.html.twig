{% extends 'base_page.html.twig' %}

{% block body_title %} <h1>Crear Nuevo Curso</h1> {% endblock %}
{% block body_content -%}

    {{ form_start(form)}}
	{{ form_row(form.nombreCurso)}}
	{{ form_row(form.sede)}}
	{{ form_row(form.carreras)}}
	{{ form_row(form.periodo)}}

	
	{{ form_row(form.year, { 'style': 'inline' })}}
	


    {{ form_end(form)}}

        <ul class="record_actions">
    <li>
        <a class="btn btn-primary" href="{{ path('curso') }}">
            Regresar
        </a>
    </li>
</ul>
{% endblock body_content %}
